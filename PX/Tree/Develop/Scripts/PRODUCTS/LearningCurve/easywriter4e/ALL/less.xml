<?xml version="1.0" encoding="utf-8"?>
<batch>
	<request cmd="updatecourses">
		<requests>
			<course courseid="-courseid-" domainid="-domainid-" title="-title-" reference="">
				<data>     
					<cssparameters>
                        <param name="Typecolor" value="#5062a4" />
                        <param name="Bordercolor" value="#026dbf" />
                        <param name="Calloutcolor" value="#026dbf" />
                        <param name="Bodytextcolor" value="#5f8d75" />
                        <param name="Columnbackgroundcolor" value="#fcf77e" />
                        <param name="bookcover" value="style/images/lc-easywriter4e-cover.jpg" />
                        <param name="course-banner-logo" value="style/images/lc-easywriter4e-logo.png" />
                        <param name="course-title-image" value="style/images/lc-easywriter4e-banner.png" />
                    </cssparameters>
				</data>
			</course>
		</requests>
	</request>
</batch>