<?xml version="1.0" encoding="utf-8"?>
<batch>
	<request cmd="updatecourses">
		<requests>
			<course courseid="-courseid-" domainid="-domainid-" title="-title-" reference="">
				<data>     
					<cssparameters>
                        <param name="Typecolor" value="#1e5ac8" />
                        <param name="Bordercolor" value="#757e5e" />
                        <param name="Calloutcolor" value="#757e5e" />
                        <param name="Bodytextcolor" value="#505050" />
                        <param name="Columnbackgroundcolor" value="#fbfcf2" />
                        <param name="bookcover" value="style/images/strayer-lc-cover.jpg" />
                        <param name="course-banner-logo" value="style/images/lc-strayer-logo.png" />
                        <param name="course-title-image" value="style/images/lc-strayer-banner.png" />
