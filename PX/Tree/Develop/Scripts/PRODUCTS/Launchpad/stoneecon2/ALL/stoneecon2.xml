<?xml version="1.0" encoding="utf-8"?>
<batch>
	<request cmd="updatecourses">
		<requests>
			<course courseid="-courseid-" domainid="-domainid-" title="-title-" reference="">
				<data>
					<bfw_rss_feeds>
						<feed name="The Economist" url="http://www.economist.com/feeds/print-sections/102/briefings2.xml" />
					</bfw_rss_feeds>              
					<cssparameters>
						<param name="Gradienttopcolor" value="#1d0002" />
						<param name="Gradientbottomcolor" value="#3b0005" />
						<param name="Typecolor" value="#244D9D" />
						<param name="TitleColor" value="#FFBF00" />  
						<param name="Columnborder" value="#634043" />
						<param name="bookcover" value="style/images/LoginPageCovers/Stoneecon2_entryPage.png" />
						<param name="course-banner-logo" value="style/images/Stoneecon2Portal_header_logo1.png" />
						<param name="course-title-image" value="style/images/Stoneecon2Portal_header_logo2.png" />
					</cssparameters>
				</data>
			</course>
		</requests>
	</request>


	<request cmd="putitems">
		<requests>
			<item itemid="PX_PRODUCT_GETTING_STARTED" entityid="-entityid-">
				<data>
					<type>CustomActivity</type>
					<parent>PX_ROOT</parent>
					<title>Everyday Writer</title>
					<abbreviation>ecw</abbreviation>
					<description>&lt;style type=&quot;text/css&quot;&gt;.action-buttons a {border-radius: 7px 7px 7px 7px; color: #FFFFFF; font-weight: bold; margin: 6px 0; padding: 4px 6px; text-decoration: none; font-size:12px; clear:both; float:left; background-color:#008ECC; width:170px; line-height:18px; text-align:center;}&lt;/style&gt;&lt;div class=&quot;action-buttons&quot; style=&#39;float:right;margin-top:50px;margin-right:20px&#39;&gt;&lt;a href=&quot;[[viewcourses]]&quot; class=&quot;ecom-btns&quot;&gt;View Your Courses&lt;span class=&quot;icon&quot;&gt;&lt;/span&gt;&lt;/a&gt;&lt;a href=&quot;[[createcourse]]&quot; class=&quot;ecom-btns [[createcoursestyles]]&quot;&gt;Create a New Course&lt;span class=&quot;icon&quot;&gt;&lt;/span&gt;&lt;/a&gt;&lt;a href=&quot;[[questionbank]]&quot; class=&quot;ecom-btns view-courses-button fixed view-questionbank-button-hidden &quot;&gt;Question Bank Administration&lt;span class=&quot;icon&quot;&gt;&lt;a href=&quot;[[editsandbox]]&quot; class=&quot;ecom-btns view-editsandbox-button-hidden&quot;&gt;Edit Sandbox Course&lt;span class=&quot;icon&quot;&gt;&lt;/span&gt;&lt;/a&gt;&lt;div style=&#39;width:250px; clear:both; padding-top:25px;margin-left:-35px;font-size:14px;&#39;&gt;&lt;div style=&#39;border:1px solid #666; border-radius:8px; padding:8px;background-color:#eee;line-height:18px;&#39;&gt;If this is your first time accessing Launch Pad, click &quot;Create a New Course&quot; to get started.&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;div style=&#39;margin-right:300px;&#39;&gt;&lt;/div&gt;</description>
					<timetocomplete>PT1M</timetocomplete>
					<category>1</category>
					<period inherit="true">0</period>
					<groupsetid>-1</groupsetid>
					<weight>100</weight>
					<gradeentry>1</gradeentry>
					<inputtable inherit="true">0</inputtable>
					<bfw_type>ProductMarketing</bfw_type>
					<bfw_subtype>Marketing</bfw_subtype>
					<bfw_tocs>
						<bfw_toc_contents parentid="PX_ROOT" sequence="_ja" />
					</bfw_tocs>
				</data>
			</item>
			<item entityid="-entityid-" itemid="PX_PRODUCT_MARKETING">
				<data>
					<type>CustomActivity</type>
					<parent>PX_ROOT</parent>
					<title>Everyday Writer</title>
					<abbreviation>ecw</abbreviation>
					<description>&lt;div class=&quot;marketing&quot;&gt;
		  &lt;div class=&quot;course-info-widget&quot;&gt;
			&lt;div class=&quot;top&quot;&gt;
			  &lt;div class=&quot;book&quot;&gt;
			  &lt;/div&gt;
			  &lt;span class=&quot;book-title&quot;&gt;Core Economics&lt;/span&gt;
			  &lt;br/&gt;
			  &lt;span&gt;Second Edition&lt;/span&gt;
			  &lt;br/&gt;
			  &lt;span&gt;Gerald Stone&lt;/span&gt;
			  &lt;br/&gt;
			&lt;/div&gt;
		  &lt;/div&gt;
		&lt;/div&gt;</description>
					<timetocomplete>PT1M</timetocomplete>
					<category>1</category>
					<period inherit="true">0</period>
					<groupsetid>-1</groupsetid>
					<weight>100</weight>
					<gradeentry>1</gradeentry>
					<inputtable inherit="true">0</inputtable>
					<bfw_type>ProductMarketing</bfw_type>
					<bfw_subtype>Marketing</bfw_subtype>
					<bfw_tocs>
						<bfw_toc_contents parentid="PX_ROOT" sequence="_ja" />
					</bfw_tocs>
				</data>
			</item>
		</requests>
	</request>

	<!--Start Page zones and widgets-->
	<request cmd="putitems">
		<requests>
			<item itemid="PX_HOME_FACEPLATE_START_ZONE_LEFT" entityid="-entityid-">
				<data>
					<parent>PX_HOME_FACEPLATE_START_ZONES</parent>
					<sequence>a</sequence>
					<title>PX_HOME_FACEPLATE_START_ZONE_LEFT</title>
					<abbreviation>PXHOMEFACEPLATEZONESTARTLEFT</abbreviation>
					<completiontrigger>0</completiontrigger>
					<bfw_type>Zone</bfw_type>
					<bfw_allowed_widgets>
						<widget type="PX_FacePlate_SA_RSS_Feed_Widget" displayName="The Economist RSS Feed" />
						<widget type="PX_Custom_RSS_Feed_FP_Start" displayName="Custom RSS Feed" />
						<widget type="PX_AssignmentWidget" displayName="Upcoming Assignments" />
						<widget type="PX_Custom" displayName="Create your own" />
					</bfw_allowed_widgets>
				</data>
			</item>
			<item itemid="PX_HOME_FACEPLATE_START_ZONE_RIGHT" entityid="-entityid-">
				<data>
					<parent>PX_HOME_FACEPLATE_START_ZONES</parent>
					<sequence>a</sequence>
					<title>PX_HOME_FACEPLATE_START_ZONE_RIGHT</title>
					<abbreviation>PXHOMEFACEPLATEZONESTARTRIGHT</abbreviation>
					<completiontrigger>0</completiontrigger>
					<bfw_type>Zone</bfw_type>
					<bfw_allowed_widgets>
						<widget type="PX_FacePlate_SA_RSS_Feed_Widget" displayName="The Economist RSS Feed" />
						<widget type="PX_Custom_RSS_Feed_FP_Start" displayName="Custom RSS Feed" />
						<widget type="PX_AssignmentWidget" displayName="Upcoming Assignments" />
						<widget type="PX_Custom" displayName="Create your own" />
					</bfw_allowed_widgets>
				</data>
			</item>
			<item itemid="PX_FacePlate_SA_RSS_Feed_Widget" entityid="-entityid-">
				<data>
					<type>CustomActivity</type>
					<parent>default_parent</parent>
					<sequence>g</sequence>
					<title>The Economist</title>
					<abbreviation>The Economist</abbreviation>
					<category>0</category>
					<completiontrigger>0</completiontrigger>
					<bfw_type>Widget</bfw_type>
					<bfw_subtype>PX_ScientificAmerican_RSS_Feed_Compact</bfw_subtype>
					<bfw_display_flags>
						<display>instructor</display>
						<display>student</display>
						<display>ebook</display>
						<display>lms</display>
					</bfw_display_flags>
					<bfw_widget_callbacks>
						<method name="View" controller="RSSFeedWidget" action="CompactViewFPStart" fne="true" />
					</bfw_widget_callbacks>
					<bfw_widget_template>PX_FacePlate_SA_RSS_Feed_Widget</bfw_widget_template>
					<bfw_properties>
						<bfw_property name="bfw_feed_items_to_show" type="String">3</bfw_property>
						<bfw_property name="bfw_feed_name" type="String">The Economist</bfw_property>
						<bfw_property name="bfw_syllabusfilter" type="String" />
						<bfw_property name="bfw_cssclass" type="String">custom-rss-widget summary</bfw_property>
						<bfw_property name="bfw_iscollapsed" type="Boolean">false</bfw_property>
						<bfw_property name="bfw_targetid" type="String" />
						<bfw_property name="bfw_ismultipleallowed" type="Boolean">false</bfw_property>
						<bfw_property name="bfw_feedurl" type="String">http://www.economist.com/topics/economics/index.xml</bfw_property>
						<bfw_property name="bfw_feeddescription" type="String">Additional HTML text required for Scientific American and other premade RSS feeds</bfw_property>
					</bfw_properties>
					<bfw_tocs>
						<bfw_toc_contents parentid="PX_WIDGETS" sequence="_p" />
					</bfw_tocs>
					<meta-bfw_assigned>false</meta-bfw_assigned>
				</data>
			</item>
			<item itemid="PX_FacePlate_SA_RSS_Feed_Widget_Default" entityid="-entityid-">
				<data>
					<type>CustomActivity</type>
					<parent>PX_HOME_FACEPLATE_START_ZONE_LEFT</parent>
					<sequence>g</sequence>
					<title>The Economist</title>
					<abbreviation>The Economist</abbreviation>
					<category>0</category>
					<completiontrigger>0</completiontrigger>
					<bfw_type>Widget</bfw_type>
					<bfw_subtype>PX_ScientificAmerican_RSS_Feed_Compact</bfw_subtype>
					<bfw_display_flags>
						<display>instructor</display>
						<display>student</display>
						<display>ebook</display>
						<display>lms</display>
					</bfw_display_flags>
					<bfw_widget_callbacks>
						<method name="View" controller="RSSFeedWidget" action="CompactViewFPStart" fne="true" />
					</bfw_widget_callbacks>
					<bfw_widget_template>PX_FacePlate_SA_RSS_Feed_Widget</bfw_widget_template>
					<bfw_properties>
						<bfw_property name="bfw_feed_items_to_show" type="String">3</bfw_property>
						<bfw_property name="bfw_feed_name" type="String">The Economist</bfw_property>
						<bfw_property name="bfw_syllabusfilter" type="String" />
						<bfw_property name="bfw_cssclass" type="String">custom-rss-widget summary</bfw_property>
						<bfw_property name="bfw_iscollapsed" type="Boolean">false</bfw_property>
						<bfw_property name="bfw_targetid" type="String" />
						<bfw_property name="bfw_ismultipleallowed" type="Boolean">false</bfw_property>
						<bfw_property name="bfw_feedurl" type="String">http://www.economist.com/topics/economics/index.xml</bfw_property>
						<bfw_property name="bfw_feeddescription" type="String">  Additional HTML text required for Scientific American and other premade RSS feeds</bfw_property>
					</bfw_properties>
					<bfw_tocs>
						<bfw_toc_contents parentid="PX_WIDGETS" sequence="_p" />
					</bfw_tocs>
					<meta-bfw_assigned>false</meta-bfw_assigned>
				</data>
			</item>	  
		</requests>


	</request>

</batch>