      <!-- Page definition for the Xbook page -->
      <item itemid="PX_XBOOK_TAB" entityid="-entityid-">
        <data>
          <parent>PX_PAGES</parent>
          <sequence>a</sequence>
          <title>PX_XBOOK_TAB</title>
          <bfw_page_editable>false</bfw_page_editable>
          <abbreviation>XBOOKTAB</abbreviation>
          <completiontrigger>0</completiontrigger>
        </data>
      </item>
      <item itemid="PX_XBOOK_TAB_ZONES" entityid="-entityid-">
        <data>
          <parent>PX_XBOOK_TAB</parent>
          <sequence>a</sequence>
          <title>PX_XBOOK_TAB_ZONES</title>
          <abbreviation>XBOOKTABZONES</abbreviation>
          <completiontrigger>0</completiontrigger>
        </data>
      </item>
