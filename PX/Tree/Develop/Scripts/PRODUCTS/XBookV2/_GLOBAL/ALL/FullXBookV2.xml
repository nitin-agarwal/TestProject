            <bfw_property name="bfw_showbrowsemoreresource" type="string">false</bfw_property>
            <bfw_property name="bfw_opencontentonclick" type="string">false</bfw_property>
            <bfw_property name="bfw_showexpandiconatalllevels" type="Boolean">true</bfw_property>
          </bfw_properties>
          <bfw_tocs>
