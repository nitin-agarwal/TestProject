<batch>
  <!-- Following command updates the product course with the correct metadata values -->
  <request cmd="updatecourses">
    <requests>
      <course courseid="-entityid-" domainid="-domainid-" type="Range">
        <data>
          <enableloadingscreen>false</enableloadingscreen>
          <meta-available-question-data>
            <coreconcept  filterable='true' friendlyname='Core Concept'/>
            <difficulty filterable='true' searchterm='difficulty:'  friendlyname='Difficulty'/>
            <cognitivelevel  filterable='true' friendlyname='Cognitive Level' />
            <bloomdomain filterable='true' friendlyname="Bloom's level"/>
            <suggesteduse filterable='true' friendlyname="Suggested Use"/>
            <guidance filterable='false' />
          </meta-available-question-data>
          <CourseTimeZone>Eastern Standard Time</CourseTimeZone>
          <gradetables>
            <table id="0" name="A,B,C,D,F" scale="Percent">
              <bracket grade="A" score="0.9" />
              <bracket grade="B" score="0.8" />
              <bracket grade="C" score="0.7" />
              <bracket grade="D" score="0.6" />
              <bracket grade="F" score="0" />
            </table>
            <table id="1" name="A+,A,A-,...,F" scale="Percent">
              <bracket grade="A+" score="0.97" />
              <bracket grade="A" score="0.93" />
              <bracket grade="A-" score="0.9" />
              <bracket grade="B+" score="0.87" />
              <bracket grade="B" score="0.83" />
              <bracket grade="B-" score="0.8" />
              <bracket grade="C+" score="0.77" />
              <bracket grade="C" score="0.73" />
              <bracket grade="C-" score="0.7" />
              <bracket grade="D+" score="0.67" />
              <bracket grade="D" score="0.63" />
              <bracket grade="D-" score="0.6" />
              <bracket grade="F" score="0" />
            </table>
            <table id="2" name="P/F" scale="Percent">
              <bracket grade="P" score="0.7" />
              <bracket grade="F" score="0" />
            </table>
          </gradetables>
          <allowstaticcopy>true</allowstaticcopy>
          <allowderivativechildcopy>true</allowderivativechildcopy>
          <allowderivativesiblingcopy>true</allowderivativesiblingcopy>
          <bfw_assessment_style>
            <rubric_management_style>
              <show_preview_on_left>true</show_preview_on_left>
              <show_edit_on_left>true</show_edit_on_left>
              <show_view_alignments>true</show_view_alignments>
              <show_edit_on_right>false</show_edit_on_right>
              <show_preview_on_right>false</show_preview_on_right>
              <show_left_column>true</show_left_column>
              <show_right_column>true</show_right_column>
              <rubric_alignment_views>
                <assignment>true</assignment>
                <eportfolio />
                <rubric>true</rubric>
              </rubric_alignment_views>
            </rubric_management_style>
            <objective_management_style>
              <show_left_column>true</show_left_column>
              <show_right_column>true</show_right_column>
              <show_edit_on_left>true</show_edit_on_left>
              <show_edit_on_right>false</show_edit_on_right>
              <show_view_alignments>true</show_view_alignments>
              <learningobj_alignment_views>
                <assignment>true</assignment>
                <eportfolio />
                <objective>true</objective>
              </learningobj_alignment_views>
            </objective_management_style>
          </bfw_assessment_style>
          <groupsets>
            <groupset id="9999" title="Individual Group" prefix="Individual Group" algorithm="Random" />
          </groupsets>
          <bfw_search>
            <searchcategory searchable="true" title="Ebook : Chapters, Headings, Topics" metadata="ebook" />
            <searchcategory searchable="true" title="Miscellaneous Content" metadata="other" />
            <searchcategory searchable="false" title="Exclude" metadata="exclude" />
          </bfw_search>
          <bfw_properties>
            <bfw_property name="bfw_infinite_scroll" type="string">true</bfw_property>
            <bfw_property name="bfw_obtain_bh_auth_cookie" type="bool">true</bfw_property>
            <bfw_property name="bfw_initialize_user" type="string">true</bfw_property>
            <bfw_property name="bfw_product_type" type="string">xbookv2</bfw_property>
          </bfw_properties>
          <bfw_tab_settings>
            <view_tab>
              <show_policies>true</show_policies>
              <show_assignment_details>false</show_assignment_details>
              <show_rubrics>false</show_rubrics>
              <show_learning_objectives>false</show_learning_objectives>
            </view_tab>
            <assign_tab>
              <show_make_gradeable>true</show_make_gradeable>
              <show_allow_late_submissions>false</show_allow_late_submissions>
              <show_schedule_reminder>false</show_schedule_reminder>
              <show_subcontent_creation>false</show_subcontent_creation>
              <show_assigned_on_same_day>true</show_assigned_on_same_day>
              <show_completion_category>true</show_completion_category>
              <show_gradebook_category>true</show_gradebook_category>
              <show_syllabus_category>true</show_syllabus_category>
              <show_points_possible>true</show_points_possible>
              <show_send_reminder>false</show_send_reminder>
            </assign_tab>
          </bfw_tab_settings>
          <bfw_allow_activation>false</bfw_allow_activation>
          <bfw_course_type>XBookV2</bfw_course_type>
          <meta-bfw_course_type>XBookV2</meta-bfw_course_type>
          <bfw_product_type>xbookv2</bfw_product_type>
          <bfw_course_home_page>PX_XBOOK_DEFAULT</bfw_course_home_page>
          <bfw_course_home_page_start>PX_XBOOK_DEFAULT</bfw_course_home_page_start>
          <bfw_hide_student_view_in_header>false</bfw_hide_student_view_in_header>
          <meta-book-isbn>000000000000X</meta-book-isbn>
          <sourceid>6651</sourceid>
          <bfw_template_learningobjectives>
            <learningobjectives>
            </learningobjectives>
          </bfw_template_learningobjectives>
          <bfw_allow_activation>true</bfw_allow_activation>

          <!--Dashboard Settings -->
          <bfw_dashboard_settings>
            <bfw_dashboard_course_home_page>PX_DASHBOARD_1COLUMN</bfw_dashboard_course_home_page>
            <bfw_program_dashboard_course_home_page>PX_PROGRAM_DASHBOARD_1COLUMN</bfw_program_dashboard_course_home_page>
            <instructor_dashboard>true</instructor_dashboard>
            <program_dashboard>true</program_dashboard>
          </bfw_dashboard_settings>
          <isdashboardactive>true</isdashboardactive>
          <!-- END Dashboard Settings -->
          <bfw_enrollment_switch>true</bfw_enrollment_switch>
          <bfw_course_domain_switch>true</bfw_course_domain_switch>
          <bfw_generic_course>false</bfw_generic_course>
          <bfw_isloadstartoninit>false</bfw_isloadstartoninit>
        </data>
      </course>
    </requests>
  </request>

  <!-- loading the default marketing and welcome screens which will be over written by the course specific scripts -->
  <request cmd="putitems">
    <requests>
      <item itemid="PX_PRODUCT_ACTIVATION_TEXT" entityid="-entityid-">
        <data>
          <description>
            &lt;div class=&quot;customerCourseActivationWrapper&quot;&gt;
            &lt;div class=&quot;alertIconWrapper&quot;&gt;
            &lt;span class=&quot;alertIcon&quot;&gt;&lt;/span&gt;
            &lt;/div&gt;
            &lt;div class=&quot;customcourseactivation&quot;&gt;
            &lt;div class=&quot;activatetext&quot;&gt;
            Click below to activate once you&#39;re ready to distribute the course URL to your class
            &lt;/div&gt;
            &lt;div class=&quot;activatelinks&quot;&gt;
            &lt;span class=&quot;activationlink&quot;&gt;[[Activation]]&lt;/span&gt;
            &lt;span class=&quot;templatelink&quot;&gt;[[SaveAsTemplate]]&lt;/span&gt;
            &lt;/div&gt;
            &lt;/div&gt;
            &lt;/div&gt;
          </description>
        </data>
      </item>
    </requests>
  </request>

  <!-- Creates necessary folders and other course structures including page definitions for XBOOK-->
  <request cmd="putitems">
    <requests>
      <!-- Page definition for the default page -->
      <item itemid="PX_XBOOK_DEFAULT" entityid="-entityid-">
        <data>
          <parent>PX_PAGES</parent>
          <sequence>a</sequence>
          <title>PX_XBOOK_DEFAULT</title>
          <bfw_page_editable>false</bfw_page_editable>
          <abbreviation>XBOOKDEFAULT</abbreviation>
          <completiontrigger>0</completiontrigger>
        </data>
      </item>
      <item itemid="PX_XBOOK_DEFAULT_ZONES" entityid="-entityid-">
        <data>
          <parent>PX_XBOOK_DEFAULT</parent>
          <sequence>a</sequence>
          <title>PX_XBOOK_DEFAULT_ZONES</title>
          <abbreviation>XBOOKDEFAULTZONES</abbreviation>
          <completiontrigger>0</completiontrigger>
        </data>
      </item>
      <item itemid="PX_XBOOK_DEFAULT_ZONE_1" entityid="-entityid-">
        <data>
          <parent>PX_XBOOK_DEFAULT_ZONES</parent>
          <sequence>a</sequence>
          <title>PX_XBOOK_DEFAULT_ZONE_1</title>
