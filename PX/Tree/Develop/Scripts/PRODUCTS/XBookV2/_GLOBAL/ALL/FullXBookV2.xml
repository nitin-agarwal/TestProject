      <item itemid="PX_XBOOK_DEFAULT_ZONE_2" entityid="-entityid-">
        <data>
          <parent>PX_XBOOK_DEFAULT_ZONES</parent>
          <sequence>b</sequence>
          <title>PX_XBOOK_DEFAULT_ZONE_2</title>
          <abbreviation>XBOOKDEFAULTZONE2</abbreviation>
