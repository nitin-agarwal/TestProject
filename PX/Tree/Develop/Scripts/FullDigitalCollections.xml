        <abbreviation>lcw</abbreviation>
        <description>&lt;div class="welcome"&gt; &lt;div class="course-info-widget"&gt; &lt;div class="top"&gt; &lt;h1&gt;Welcome, Instructor!&lt;/h1&gt; &lt;span&gt; It’s easy to find, customize, and assign primary source projects and documents that work for your course. Your students will only see the projects that you have assigned.&lt;/span&gt; &lt;h2&gt;Get started:&lt;/h2&gt; &lt;ol&gt; &lt;li&gt;Browse the Primary Source Projects.&lt;/li&gt; &lt;li&gt;Edit them to match your course.&lt;/li&gt; &lt;li&gt;Assign them to your students.&lt;/li&gt; &lt;/ol&gt; &lt;a class="activation-link register-lc"&gt;Enable Student Registration &lt;span class="icon"&gt;&lt;/span&gt;&lt;/a&gt; &lt;/div&gt; &lt;div class="bottom"&gt; &lt;ul class="tutorial-links"&gt; &lt;li class="header-row"&gt;&lt;span rel=""&gt;Instructor Support&lt;/span&gt;&lt;/li&gt; &lt;li &gt;&lt;a href="#" target="_blank"&gt;&lt;span rel=""&gt;How to get started&lt;/span&gt;&lt;span class="tut-link-arrow"&gt;&lt;/span&gt;&lt;/a&gt;&lt;/li&gt; &lt;li&gt;&lt;a href="#" target="_blank"&gt;&lt;span rel=""&gt;FAQs&lt;/span&gt;&lt;span class="tut-link-arrow"&gt;&lt;/span&gt;&lt;/a&gt;&lt;/li&gt; &lt;/ul&gt; &lt;/div&gt; &lt;/div&gt; &lt;/div&gt;</description>
        <timetocomplete>PT1M</timetocomplete>
        <category>0</category>
        <period inherit="true">0</period>
        <groupsetid>-1</groupsetid>
        <weight>100</weight>
        <gradeentry>1</gradeentry>
        <inputtable inherit="true">0</inputtable>
        <completiontrigger>0</completiontrigger><bfw_type>Widget</bfw_type><bfw_subtype>PX_Custom</bfw_subtype><bfw_display_flags>
